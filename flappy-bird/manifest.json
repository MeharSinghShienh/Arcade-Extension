{
  "manifest_version": 3,
  "name": "Classic Flappy Bird Offline",
  "description": "A Fun Game of Flappy Bird",
  "version": "1.0.0",
  "icons": {
    "16": "./img/flappybirdicon16.png",
    "32": "./img/flappybirdicon32.png",
    "48": "./img/flappybirdicon48.png",
    "128": "./img/flappybirdicon128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Flappy Bird",
    "default_icon": "./img/flappybirdicon128.png"
  }
